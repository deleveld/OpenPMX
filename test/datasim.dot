digraph G {
	size="5,4!"
	overlap=false
	splines=true
#   newrank=true;

	fontname="Helvetica,Arial,sans-serif"
	fontsize=14
	node [fontname="Helvetica,Arial,sans-serif"]
	edge [fontname="Helvetica,Arial,sans-serif"]
    node [shape=box];

    dose -> v1
	v1 -> v2 [label="KA"]
	v2 -> elim [label="KE"]
	v2 -> obs [style=dotted]

	obs[label="Prediction",color="white"]

	dose [label="Dose",color="white"]
	v1 [label="Depot"]
	v2 [label="V"]
	elim [style=invis]

	{ rank=same; dose; v1; v2; elim; }
}
