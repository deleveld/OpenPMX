digraph G {
	size="5,4!"
	overlap=false
	splines=true
#   newrank=true;

	fontname="Helvetica,Arial,sans-serif"
	fontsize=14
	node [fontname="Helvetica,Arial,sans-serif"]
	edge [fontname="Helvetica,Arial,sans-serif"]
    node [shape=box];

    dose -> v1
	v1 -> v2 [label="Q2"]
	v1 -> v3 [label="Q3"]
	v1 -> elim [label="CL"]
	v1-> prediction [style=dotted]

	prediction [label="Prediction",color="white"]
	dose [label="Dose",color="white"]
	v1 [label="V1"]
	v2 [label="V2"]
	v3 [label="V3"]
	elim [style=invis]

	{ rank=same; dose; v1; elim; }
}
